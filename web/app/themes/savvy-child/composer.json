{
    "name": "upstatement/timber-starter-theme",
    "description": "Starter theme to build a Timber theme",
    "type": "wordpress-theme",
    "minimum-stability": "stable",
    "authors": [
        {
            "email": "jared@upstatement.com",
            "name": "jarednova"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "timber/timber": "1.21.0",
        "phospr/fraction": "dev-master",
        "buzzingpixel/twig-switch": "1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.16|6.*"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "autoload": {
        "psr-4": {
            "SavvyTheme\\": "../savvy/app",
            "ScTheme\\": "./app"
        }
    }
}